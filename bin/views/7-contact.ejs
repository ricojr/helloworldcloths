            <div class="col-md-6">
              <h2 class=" mt-3 font-weight-bold text-white">
                Drop us message
              </h2>
              <form action="">
                <div class="row">
                  <div class="col-lg-6">
                    <div class="form-group">
                      <input
                        type="text"
                        class="form-control mt-2"
                        placeholder="Name"
                        required
                      />
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <input
                        type="text"
                        class="form-control mt-2"
                        placeholder="Family name"
                        required
                      />
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <input
                        type="email"
                        class="form-control mt-2"
                        placeholder="Email"
                        required
                      />
                    </div>
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <input
                        type="number"
                        class="form-control mt-2"
                        placeholder="Mobile phone"
                        required
                      />
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group">
                      <textarea
                        class="form-control"
                        id="exampleFormControlTextarea1"
                        placeholder="message"
                        rows="3"
                        required
                      ></textarea>
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group">
                      <div class="form-check">
                        <input
                          class="form-check-input"
                          type="checkbox"
                          value=""
                          id="invalidCheck2"
                          required
                        />
                        <label class="form-check-label" for="invalidCheck2">
                          Accept the conditions
                        </label>
                      </div>
                    </div>
                  </div>
                  <div class="col-12">
                    <button class="btn btn-light" type="submit">Send</button>
                  </div>
                </div>
